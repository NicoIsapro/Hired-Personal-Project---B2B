<!DOCTYPE html>
<html lang="en">

<head>
    {% embed('@WebBundle/Resources/views/Layouts/admin_assets.html.twig') %}
    {% endembed %}
    <!-- Title Page-->
    <title>Signup - {{ WebInfos.0.webtitle }}</title>
</head>

<body class="animsition">
    <div class="page-wrapper">
        <div class="page-content--bge5">
            <div class="container">
                <div class="login-wrap">
                    <div class="login-content">
                        <div class="login-logo">
                            <a href="#">
                                <h1> {{ WebInfos.0.webtitle }} </h1>
                            </a>
                        </div>
                        <div class="login-form">
                          {{ form_start(form) }}
                            {{ form_errors(form) }}

                            <div class="form-group">
                              {{ form_row(form.company) }}
                            </div>
                            <div class="form-group">
                              {{ form_row(form.surname) }}
                            </div>
                            <div class="form-group">
                            {{ form_row(form.name) }}
                          </div>
                          <div class="form-group">
                            {{ form_row(form.username) }}
                          </div>
                          <div class="form-group">
                            {{ form_row(form.email) }}
                          </div>
                          <div class="form-group">
                            {{ form_row(form.password) }}
                          </div>
                            <button class="au-btn au-btn--block au-btn--green m-b-20" type="submit">ask a register</button>
                          {{ form_end(form) }}
                              <div class="social-login-content">
                                    <div class="social-button">
                                        <button class="au-btn au-btn--block au-btn--blue2">register with LinkedIn</button>
                                    </div>
                                </div>
                            <div class="register-link">
                                <p>
                                    Already have account?
                                    <a href="/login">Sign In</a>
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        {% embed('@WebBundle/Resources/views/Layouts/admin_js.html.twig') %}
        {% endembed %}
    </div>
</body>
</html>
<!-- end document-->
